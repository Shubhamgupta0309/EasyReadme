# Server Environment Variables
PORT=5000
NODE_ENV=development

# GitHub API (Optional - for increased rate limits)
GITHUB_TOKEN=your_github_personal_access_token

# Google Gemini AI API (Get from: https://makersuite.google.com/app/apikey)
# Leave as 'demo-key' to use template-based generation
GEMINI_API_KEY=demo-key

# CORS Configuration
CLIENT_URL=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100
